import java.util.*;

class mockccc14j1{
  
  static Scanner input = new Scanner (System.in);
  
  public static void main (String args[]) {
    
    int a, b, r;
    
    a= input.nextInt();
    b= input.nextInt();
    r=input.nextInt();
    
    if (a>r)
      System.out.println("Bob overdoses on day 1.");
    else if ((a*0.5+b)>r)
    System.out.println("Bob overdoses on day 2.");
    else 
      System.out.println("Bob never overdoses.");
  }
}
   
