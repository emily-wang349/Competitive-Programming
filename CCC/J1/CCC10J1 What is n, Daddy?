import java.util.*;

class ccc10j1 {
  static Scanner input = new Scanner (System.in);
  
  public static void main (String args [])
  {
    int n;
    n=input.nextInt();
    
    int counter =0;
    
    for (int i=0; i<=10; i++)
    {
      if (n==i+(n-i) && (n-i)>=0 && i<=5 && (n-i)<=5)
        counter++;
    }
    System.out.println((counter+1)/2);
    
  
  }
}
