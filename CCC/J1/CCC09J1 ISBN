import java.util.*;

class ccc09j1 {
  static Scanner input = new Scanner (System.in);
  
  public static void main (String args[]) {
    
     int in [] = new int [3];
     for (int i=0; i<3; i++)
       in [i] =input.nextInt();
     
     int sum = 91+in[0]+in[1]*3+in[2];
     
    System.out.println("The 1-3-sum is "+sum);
  }
}
