import java.util.*;

public class firebend {

	static Scanner input = new Scanner(System.in);

	public static void main(String[] args) {
		int n = input.nextInt();
		int[] a = new int[n];
		for (int i = 0; i < n; i++)
			a[i] = input.nextInt();
		int sum = 0;
		for (int i = 0; i < n; i++)
			sum += Math.abs(a[i]);
		System.out.println(sum);
	}

}
