import java.util.*;
class p287ex3{
  static Scanner input = new Scanner(System.in);
  
  public static void main (String args[])
  {
    int a = input.nextInt();
    double b [] = new double [a];
    
    for (int i=0; i<a; i++)
      b [i] = input.nextDouble();
    
   double min = b[0];
    
    for (int i=1; i<a; i++)
    {
      if (min>b[i])
        min= b[i];
    }
    
    System.out.println(min);
    
  }
}
