import java.util.*;

public class p299ex3 {

	public static void main(String[] args) {
		Scanner input = new Scanner (System.in);
		
		int n = input.nextInt();
		input.nextLine();
		
		String a [] = new String [n];
		
		for (int i = 0; i<n; i++)
		{
			a [i] = input.nextLine();

		}
		for (int i = 0; i<n; i++)
		{
			System.out.println(a[i].toLowerCase());
		}
	}

}
