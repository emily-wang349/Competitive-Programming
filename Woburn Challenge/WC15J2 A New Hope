import java.util.*;

class wc152j1 {
  static Scanner input = new Scanner(System.in);
  
  public static void main (String agrs[])
  {
    int a = input.nextInt();
    System.out.print("A long time ago in a galaxy ");
    
    for (int i=0; i<a-1; i++)
    {
      System.out.print("far, ");
    }
    System.out.print("far away...");
  }
}
